{
    "version": 2,
    "builds": [
      {
        "src": "src/server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/socket.io",
        "dest": "src/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "src/server.js"
      }
    ],
    "env": {
      "PORT": "3000"
    }
}